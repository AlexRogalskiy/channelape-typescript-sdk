{
  "name": "@channelape-inc/channelape-typescript-sdk",
  "version": "1.0.0",
  "description": "A client for interacting with ChannelApe's API",
  "main": "dist/ChannelapeClient.js",
  "typings": "dist/ChannelapeClient.d.ts",
  "scripts": {
    "postinstall": "npm run compile",
    "prepublishOnly": "npm run compile",
    "compile": "tsc -p . --declaration",
    "watch": "tsc --watch",
    "unit-test": "mocha",
    "watch-unit-test": "mocha --watch",
    "test": "npm run unit-test",
    "pretest": "npm run lint",
    "posttest": "npm run mutate && npm run cover",
    "cover": "nyc npm run unit-test",
    "mutate": "stryker run",
    "lint": "tslint --project ."
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChannelApe/channelape-typescript-sdk.git"
  },
  "keywords": [
    "channelape",
    "sdk",
    "javascript",
    "typescript",
    "ecommerce",
    "rest-client"
  ],
  "author": "ChannelApe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ChannelApe/channelape-typescript-sdk/issues"
  },
  "homepage": "https://github.com/ChannelApe/channelape-typescript-sdk#readme",
  "contributors": [
    {
      "name": "RJ Davis",
      "email": "rjdavis@channelape.com"
    },
    {
      "name": "Ryan Kazokas",
      "email": "rkazokas@channelape.com"
    },
    {
      "name": "Frank Mazzarella",
      "email": "fmazzarella@channelape.com"
    }
  ],
  "dependencies": {
    "@types/log4js": "^2.3.5",
    "@types/node": "^9.6.4",
    "@types/q": "^1.5.0",
    "guid": "0.0.12",
    "leaky-bucket": "2.1.1",
    "log4js": "^2.3.10",
    "node-rest-client": "3.1.0",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "@types/sinon": "^4.3.0",
    "app-root-path": "^2.0.1",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "3.4.2",
    "mocha-typescript": "^1.1.12",
    "nock": "^9.2.5",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "source-map-support": "^0.5.4",
    "stryker": "^0.17.0",
    "stryker-api": "^0.12.0",
    "stryker-html-reporter": "^0.11.4",
    "stryker-mocha-framework": "^0.7.2",
    "stryker-mocha-runner": "^0.10.8",
    "stryker-typescript": "^0.8.3",
    "ts-node": "^4.1.0",
    "tsc": "1.20150623.0",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.4.2",
    "typescript": "2.5.3",
    "typescript-eslint-parser": "3.0.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "node_modules/**",
      "dist/**",
      "reports/**",
      "test/**",
      "**/*.d.ts",
      "**/*.conf.js"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "./reports/lcov"
  }
}
