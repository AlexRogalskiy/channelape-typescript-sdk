{
  "name": "@channelape-inc/channelape-typescript-sdk",
  "version": "1.0.0",
  "description": "An javascript client for interacting with ChannelApe's API",
  "main": "dist/channelape-client.js",
  "typings": "dist/channelape-client.d.ts",
  "scripts": {
    "postinstall": "node ./node_modules/typescript/bin/tsc",
    "start": "node ./dist/channelape-client.js",
    "prepublishOnly": "npm run compile",
    "compile": "tsc -p . --declaration",
    "dev": "tsc --watch & nodemon dist",
    "test": "node ./node_modules/mocha/bin/mocha test/**/*.spec.* --recursive --compilers ts:ts-node/register",
    "cover": "./node_modules/.bin/nyc npm t",
    "mutate": "./node_modules/.bin/stryker run",
    "lint": "./node_modules/.bin/tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChannelApe/channelape-typescript-sdk.git"
  },
  "keywords": [
    "channelape",
    "sdk",
    "javascript",
    "typescript",
    "ecommerce",
    "rest-client"
  ],
  "author": "ChannelApe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ChannelApe/channelape-typescript-sdk/issues"
  },
  "homepage": "https://github.com/ChannelApe/channelape-typescript-sdk#readme",
  "contributors": [
    {
      "name": "RJ Davis",
      "email": "rjdavis@channelape.com"
    },
    {
      "name": "Ryan Kazokas",
      "email": "rkazokas@channelape.com"
    },
    {
      "name": "Frank Mazzarella",
      "email": "fmazzarella@channelape.com"
    }
  ],
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "@types/sinon": "^4.1.3",
    "app-root-path": "^2.0.1",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "3.4.2",
    "mocha-typescript": "^1.1.12",
    "nock": "^9.1.4",
    "nyc": "^11.4.1",
    "sinon": "^4.3.0",
    "stryker": "^0.17.0",
    "stryker-api": "^0.12.0",
    "stryker-html-reporter": "^0.11.4",
    "stryker-mocha-framework": "^0.7.2",
    "stryker-mocha-runner": "^0.10.5",
    "stryker-typescript": "^0.8.3",
    "ts-node": "^4.1.0",
    "tsc": "1.20150623.0",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.4.2",
    "typescript": "2.5.3",
    "typescript-eslint-parser": "3.0.0"
  },
  "dependencies": {
    "@types/log4js": "^2.3.5",
    "@types/node": "^9.4.7",
    "@types/q": "^1.0.7",
    "guid": "0.0.12",
    "leaky-bucket": "2.1.1",
    "log4js": "^2.3.10",
    "node-rest-client": "3.1.0",
    "uuid": "3.1.0"
  }
}
